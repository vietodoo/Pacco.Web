@page "/identity"
@layout MainLayout
@using Pacco.Web.Areas.Identity.Components
@inject IdentityComponent Component;

<h1>Identity</h1>

<button type="button" class="btn btn-primary" onclick=@(() => Component.SignUpAsync("user1@mailinator.com", "secret", "user"))>Sign up</button>

@code {
    protected override async Task OnInitAsync()
    {
        await Component.OnInit();
    }
}